{
	"compilerOptions": {
		"target": "ES2017",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "react-jsx",
		"incremental": true,
		"plugins": [
			{
				"name": "next"
			}
		],
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts",
		".next/dev/types/**/*.ts",
		"app/(portfolio)/layout.jsx",
		"app/(portfolio)/loading.jsx",
		"app/(portfolio)/page.jsx",
		"app/actions/submit-contact-form.js",
		"app/actions/disableDraftMode.js",
		"app/actions/create-session.js",
		"app/(sanity)/studio/[[...tool]]/page.jsx",
		"app/(sanity)/studio/[[...tool]]/layout.jsx",
		"app/api/draft-mode/enable/route.js",
		"components/sections/index.js",
		"components/sections/HeroSection.jsx",
		"components/sections/ExperienceSection.jsx",
		"components/sections/EducationSection.jsx",
		"components/sections/ContactSection.jsx",
		"components/sections/ContactForm.jsx",
		"components/sections/CertificationsSection.jsx",
		"components/sections/BlogSection.jsx",
		"components/sections/AchievementsSection.jsx",
		"components/sections/AboutSection.jsx",
		"components/sections/TestimonialsSection.jsx",
		"components/sections/ServicesSection.jsx",
		"components/sections/ProfileImage.jsx",
		"components/ui/dropdown-menu.jsx",
		"components/ui/comet-card.jsx",
		"components/ui/chart.jsx",
		"components/ui/card.jsx",
		"components/ui/button.jsx",
		"components/ui/background-ripple-effect.jsx",
		"components/ui/animated-testimonials.jsx",
		"components/ui/world-map.jsx",
		"components/ui/sheet.jsx",
		"components/ui/separator.jsx",
		"components/ui/layout-text-flip.jsx",
		"components/ui/input.jsx",
		"components/ui/tooltip.jsx",
		"components/ui/spinner.jsx",
		"components/ui/skeleton.jsx",
		"components/world-map-demo.jsx",
		"components/ThemeProvider.jsx",
		"components/PortfolioContent.jsx",
		"components/FloatingDockClient.jsx",
		"components/FloatingDock.jsx",
		"components/DynamicIcon.jsx",
		"components/DisableDraftMode.jsx",
		"components/DarkModeToggle.jsx",
		"sanity/schemaTypes/siteSettings.js",
		"sanity/schemaTypes/service.js",
		"sanity/schemaTypes/project.js",
		"sanity/schemaTypes/profile.js",
		"sanity/schemaTypes/navigation.js",
		"sanity/schemaTypes/index.js",
		"sanity/schemaTypes/experience.js",
		"sanity/schemaTypes/education.js",
		"sanity/schemaTypes/contact.js",
		"sanity/schemaTypes/certification.js",
		"sanity/schemaTypes/blog.js",
		"sanity/schemaTypes/achievement.js",
		"sanity/lib/serverClient.js",
		"sanity/lib/live.js",
		"sanity/lib/image.js",
		"sanity/lib/client.js",
		"lib/utils.js",
		"lib/config.js",
		"hooks/use-mobile.js",
		"sanity/structure.js",
		"sanity/env.js",
		"sanity/schemaTypes/testimonial.js",
		"next.config.js",
		"sanity.config.js",
		"sanity.cli.js"
	],
	"exclude": ["node_modules"]
}
